{
    "name": "Node.js Dev Container",
    "dockerFile": "../Dockerfile",
    "context": "..",
    "target": "development",
    "appPort": [
      "5173:5173"
    ],
    "extensions": [
      "dbaeumer.vscode-eslint",
      "esbenp.prettier-vscode",
      "Orta.vscode-jest",
      "ms-playwright.playwright"
    ],
    "settings": {
      
    },
    "postCreateCommand": "npm install && npm run dev",
    "runArgs": [
      "--init"
    ],
    "remoteUser": "node"
  }